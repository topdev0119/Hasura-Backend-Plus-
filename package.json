{
  "name": "hasura-backend-plus",
  "license": "MIT",
  "repository": {
    "url": "git://github.com/nhost/hasura-backend-plus.git",
    "type": "git"
  },
  "version": "1.2.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify src/index.ts",
    "start": "tsc && cross-env NODE_ENV=production node -r ./tscPaths.js dist/index.js"
  },
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "@hapi/joi": "17.1.1",
    "argon2": "0.26.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.1.1",
    "graphql": "14.6.0",
    "graphql-request": "1.8.2",
    "graphql-tag": "2.10.3",
    "helmet": "3.21.3",
    "hibp": "8.0.0",
    "jsonwebtoken": "8.5.1",
    "otplib": "12.0.1",
    "qrcode": "1.4.4",
    "tsconfig-paths": "3.9.0",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.3",
    "@types/express-rate-limit": "5.0.0",
    "@types/hapi__joi": "16.0.12",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "8.3.8",
    "@types/node": "13.9.1",
    "@types/qrcode": "1.3.4",
    "@types/uuid": "7.0.1",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "root": true,
    "rules": {
      "@typescript-eslint/camelcase": "off"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint"
    ]
  },
  "eslintIgnore": [
    "dist",
    "node_modules"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js,*.ts,*.md,*.json": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleQuote": true
  }
}
